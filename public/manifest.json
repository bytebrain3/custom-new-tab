{
    "manifest_version": 3,
    "name": "Custom New Tab Art",
    "version": "1.0",
    "description": "A custom new tab page with art and links.",
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "storage"
    ],
    "icons": {
        "16": "logo.jpg",
        "48": "logo.jpg",
        "128": "logo.jpg"
    }
}